import requests

BASE = "https://dadosabertos.camara.leg.br/api/v2"

def fetch_votes(votacao_id):
    url = f"{BASE}/votacoes/{votacao_id}/votos"
    r = requests.get(
        url,
        headers={"Accept": "application/json"},
        timeout=60
    )

    if r.status_code != 200:
        return []

    j = r.json()
    return j.get("dados", [])
